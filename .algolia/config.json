{
  "index_name": "windicss-next",
  "start_urls": [
    {
      "url": "https://windicss.org/utilities/",
      "selectors_key": "utilities",
      "tags": ["utilities"],
      "page_rank": 3
    },
    {
      "url": "https://windicss.org/plugins/",
      "tags": ["plugins"],
      "page_rank": 2
    },
    {
      "url": "https://windicss.org/guide/",
      "tags": ["guide"],
      "page_rank": 1
    }
  ],
  "stop_urls": [
    ".html$"
  ],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "p.sidebar-heading.open",
        "global": true,
        "default_value": "Guide"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3, .content li kbd",
      "lvl4": ".content h4",
      "lvl5": ".content h5",
      "text": ".content p, .content li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      }
    },
    "utilities": {
      "lvl0": {
        "selector": "p.sidebar-heading.open",
        "global": true,
        "default_value": "Utilities"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3",
      "lvl4": ".content h4, .content tbody tr td:nth-child(1)",
      "text": ".content p, .content li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      }
    }
  },
  "scrape_start_urls": true,
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": [
      "lang"
    ]
  }
}
